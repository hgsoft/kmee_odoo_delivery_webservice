<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="kmee_view_delivery_carrier_form" model="ir.ui.view">
			<field name="name">kmee_delivery_webservice.carrier.form</field>
			<field name="model">delivery.carrier</field>
			<field name="type">form</field>
			<field name="inherit_id" ref="delivery.view_delivery_carrier_form"/> 
			<field name="arch" type="xml">
				<page string="Destination" position="after">
					<page string="Web Service">
						<group>
							<field name="service"/>
							<field name="service_type" />
							<field name="login" />
							<field name="password" password="True"/>
						</group>
					</page>
				</page>
			</field>
		</record>
		<record id="kmee_view_delivery_grid_form" model="ir.ui.view">
			<field name="name">kmee_delivery_webservice.grid.form</field>
			<field name="model">delivery.grid</field>
			<field name="type">form</field>
			<field name="inherit_id" ref="delivery.view_delivery_grid_form"/> 
			<field name="arch" type="xml">
				<page string="Destination" position="after">
					<page string="Web Service">
						<group>
							<field name="service"/>
							<field name="service_type" />
							<field name="login" />
							<field name="password" password="True"/>
						</group>
					</page>
				</page>
			</field>
		</record>
	</data>
</openerp>
